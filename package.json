{
  "name": "change-me",
  "private": true,
  "scripts": {
    "compile": "rm -rf ./target/ && tsc",
    "start": "yarn run compile && node --enable-source-maps ./target/index.js",
    "format": "prettier --write './**/*.{ts,tsx,js,json,md}' '.prettierrc'"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  }
}
